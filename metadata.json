  "license": "GPLv3+",
  "source": "https://github.com/hambuergaer/freimer-vmaretools",
  "project_page": "https://github.com/hambuergaer/freimer-vmaretools",
  "issues_url": "https://github.com/hambuergaer/freimer-vmaretools/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
